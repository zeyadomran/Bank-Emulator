<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<Scene xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controller">
    <AnchorPane prefHeight="200" prefWidth="200" styleClass="mainBody" stylesheets="@../resources/css/style.css">
      <children>
         <Pane prefHeight="200.0" prefWidth="200.0" styleClass="header">
            <children>
               <Label layoutX="360.0" layoutY="10.0" styleClass="logoWhite" />
            </children>
         </Pane>
         <Pane layoutX="25.0" layoutY="200.0" prefHeight="200.0" prefWidth="200.0" styleClass="mainMenuContainer">
            <children>
               <Label layoutX="291.0" layoutY="40.0" text="Edit Menu">
                  <styleClass>
                     <String fx:value="txt1" />
                     <String fx:value="txt" />
                  </styleClass>
               </Label>
               <Button layoutX="155.0" layoutY="117.0" mnemonicParsing="false" onAction="#editUsername" text="Edit Username">
                  <styleClass>
                     <String fx:value="btnBig2" />
                     <String fx:value="btn2" />
                  </styleClass>
               </Button>
               <Button layoutX="395.0" layoutY="117.0" mnemonicParsing="false" onAction="#editName" text="Edit Name">
                  <styleClass>
                     <String fx:value="btnBig2" />
                     <String fx:value="btn2" />
                  </styleClass>
               </Button>
               <Button layoutX="275.0" layoutY="205.0" mnemonicParsing="false" onAction="#mainMenuScene" text="Go Back">
                  <styleClass>
                     <String fx:value="btnBig2" />
                     <String fx:value="btn3" />
                  </styleClass>
               </Button>
            </children>
         </Pane>
      </children></AnchorPane>
</Scene>
