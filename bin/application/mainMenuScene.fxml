<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<Scene xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controller">
    <AnchorPane prefHeight="200" prefWidth="200" styleClass="mainBody" stylesheets="@../resources/css/style.css">
      <children>
         <Pane prefHeight="200.0" prefWidth="200.0" styleClass="header">
            <children>
               <Label layoutX="360.0" layoutY="10.0" styleClass="logoWhite" />
            </children>
         </Pane>
         <Pane layoutX="25.0" layoutY="200.0" prefHeight="200.0" prefWidth="200.0" styleClass="mainMenuContainer">
            <children>
               <Label fx:id="welcomeMessageMenu" layoutX="50.0" layoutY="40.0" text="Hello Zeyad Omran!">
                  <styleClass>
                     <String fx:value="txt" />
                     <String fx:value="txt1" />
                  </styleClass>
               </Label>
               <Label fx:id="balance" layoutX="50.0" layoutY="90.0" text="Balance: $ 6748.99">
                  <styleClass>
                     <String fx:value="txt" />
                     <String fx:value="txt2" />
                  </styleClass>
               </Label>
               <Button layoutX="517.0" layoutY="70.0" mnemonicParsing="false" onAction="#logout" text="Log Out">
                  <styleClass>
                     <String fx:value="btnBig" />
                     <String fx:value="btn3" />
                  </styleClass></Button>
               <Button layoutX="84.0" layoutY="140.0" mnemonicParsing="false" onAction="#deposit" text="Deposit">
                  <styleClass>
                     <String fx:value="btnBig" />
                     <String fx:value="btn2" />
                  </styleClass></Button>
               <Button layoutX="300.0" layoutY="140.0" mnemonicParsing="false" onAction="#withdraw" text="Withdraw">
                  <styleClass>
                     <String fx:value="btnBig" />
                     <String fx:value="btn2" />
                  </styleClass></Button>
               <Button layoutX="517.0" layoutY="140.0" mnemonicParsing="false" onAction="#transferScene" text="Transfer">
                  <styleClass>
                     <String fx:value="btnBig" />
                     <String fx:value="btn2" />
                  </styleClass></Button>
               <Button layoutX="84.0" layoutY="210.0" mnemonicParsing="false" onAction="#transactionsScene" text="Transactions">
                  <styleClass>
                     <String fx:value="btnBig" />
                     <String fx:value="btn2" />
                  </styleClass></Button>
               <Button layoutX="300.0" layoutY="210.0" mnemonicParsing="false" onAction="#viewMessagesScene" text="Messages">
                  <styleClass>
                     <String fx:value="btnBig" />
                     <String fx:value="btn2" />
                  </styleClass></Button>
               <Button layoutX="517.0" layoutY="210.0" mnemonicParsing="false" onAction="#editInfoScene" text="Edit Info">
                  <styleClass>
                     <String fx:value="btnBig" />
                     <String fx:value="btn2" />
                  </styleClass></Button>
            </children>
         </Pane>
      </children></AnchorPane>
</Scene>
