<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<Scene xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controller">
    <AnchorPane prefHeight="200" prefWidth="200" styleClass="mainBody" stylesheets="@../resources/css/style.css">
      <children>
         <Pane prefHeight="200.0" prefWidth="200.0" styleClass="header">
            <children>
               <Label layoutX="360.0" layoutY="10.0" styleClass="logoWhite" />
            </children>
         </Pane>
         <Pane layoutX="25.0" layoutY="200.0" prefHeight="200.0" prefWidth="200.0" styleClass="mainMenuContainer">
            <children>
               <Button defaultButton="true" layoutX="490.0" layoutY="75.0" mnemonicParsing="false" onAction="#sendTransfer" text="Send Transfer">
                  <styleClass>
                     <String fx:value="btnBig" />
                     <String fx:value="btn1" />
                  </styleClass>
               </Button>
               <Button layoutX="490.0" layoutY="180.0" mnemonicParsing="false" onAction="#mainMenuScene" text="Cancel">
                  <styleClass>
                     <String fx:value="btnBig" />
                     <String fx:value="btn3" />
                  </styleClass>
               </Button>
               <TextField fx:id="fromTransfer" editable="false" layoutX="50.0" layoutY="30.0" promptText="From:" styleClass="inputField" />
               <TextField fx:id="toTransfer" layoutX="50.0" layoutY="120.0" promptText="To:" styleClass="inputField" />
               <TextField fx:id="amountTransfer" layoutX="50.0" layoutY="210.0" promptText="Amount:" styleClass="inputField" />
            </children>
         </Pane>
      </children></AnchorPane>
</Scene>
