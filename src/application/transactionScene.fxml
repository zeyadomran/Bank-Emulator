<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<Scene xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controller">
    <AnchorPane prefHeight="200" prefWidth="200" styleClass="mainBody" stylesheets="@../resources/css/style.css">
      <children>
         <Pane prefHeight="200.0" prefWidth="200.0" styleClass="header">
            <children>
               <Label layoutX="360.0" layoutY="10.0" styleClass="logoWhite" />
            </children>
         </Pane>
         <ScrollPane layoutX="25.0" layoutY="200.0" prefHeight="200.0" prefWidth="200.0">
            <styleClass>
               <String fx:value="mainMenuContainer" />
               <String fx:value="scrollPane" />
            </styleClass>
            <content>
               <Pane fx:id="transactionPane" prefHeight="200.0" prefWidth="200.0" styleClass="scrollPaneFiller" />
            </content>
         </ScrollPane>
         <Button layoutX="325.0" layoutY="525.0" mnemonicParsing="false" onAction="#mainMenuScene" text="Go Back">
            <styleClass>
               <String fx:value="btnBig" />
               <String fx:value="btn1" />
            </styleClass>
         </Button>
      </children></AnchorPane>
</Scene>
